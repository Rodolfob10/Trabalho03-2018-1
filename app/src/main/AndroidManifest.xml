<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="pooa20181.iff.edu.br.trabalho03">

    <application
        android:name=".model.BDConfig"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".actvity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity
            android:name=".actvity.ListaOficina"
            android:label="Lista Oficina"/>

        <activity
            android:name=".actvity.ListaMecanico"
            android:label="Lista Mecanico"/>

        <activity
            android:name=".actvity.OficinaDetalhe"
            android:label="Oficina Detalhe"/>

        <activity
            android:name=".actvity.MecanicoDetalhe"
            android:label="Mecanico Detalhe"/>

        <meta-data
            android:name="com.google.android.geo.API_KEY"
            android:value="@string/google_maps_key"
            />




</application>

</manifest>